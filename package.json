{
  "name": "cellular-automata",
  "version": "3.0.0",
  "description": "An exploration of two-dimensional arrays in JavaScript.",
  "scripts": {
    "build": "parcel build src/index.html --out-dir docs --public-url /cellular-automata",
    "format": "prettier --write 'src/**/*'",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint 'src/**/*.js'",
    "lint:styles": "stylelint 'src/**/*.css'",
    "publish": "rimraf docs && npm run lint && npm run build",
    "start": "parcel src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjaminkimball/cellular-automata.git"
  },
  "author": "Benjamin Kimball <hello@benjaminkimball.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjaminkimball/cellular-automata/issues"
  },
  "homepage": "https://github.com/benjaminkimball/cellular-automata#readme",
  "dependencies": {
    "preact": "^10.1.1",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0"
  }
}
